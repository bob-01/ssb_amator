jQuery(function ($) {
    'use strict';
    var imgAssistElem = parent.document.getElementsByName("img_assist_header");
    if (null !== imgAssistElem && imgAssistElem.length > 0) {
        imgAssistElem[0].scrolling = "no";
        imgAssistElem[0].style.height = "150px";
    }
});


